generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model MovieLike {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  movieId String @unique
  user    User   @relation(fields: [userId], references: [id])
  userId  String @db.ObjectId
}

model User {
  id       String      @id @default(auto()) @map("_id") @db.ObjectId
  email    String      @unique
  password String
  movie    MovieLike[]
}
